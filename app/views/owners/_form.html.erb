
<%= form_for @owner do |f| %>
<div class="field">
  <%= f.text_field :name, placeholder: "name"  %>
</div>
<div class="field">
<%= f.text_field :address, placeholder: "address"  %>
</div>
<div class="field">
<%= f.text_field :city, placeholder: "city"  %>
</div>
<div class="field">
<%= f.hidden_field :user_id, :value => session[:user_id] %>
</div>
<div class="field">
<%= f.submit %>
</div>
<% end %>
