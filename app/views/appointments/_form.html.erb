<ul>
<%= form_for @appointment do |f| %>
<%= f.label :dog_id %>
<%= f.collection_select :dog_id, Dog.all, :id, :name %>
<%= f.label :walker_id %>
<%= f.collection_select :walker_id, Walker.all, :id, :user_id %>
<%= f.label :datetime %>
<%= f.text_field :datetime %>

<%= f.submit %>
<% end %>
</ul>


